{
  "name": "my-day-web",
  "version": "1.0.0",
  "scripts": {
    "pack": "npm run mv:index && webpack",
    "mv:index": "cp -f ./src/index.html ./build/",
    "watch": "npm run mv:index && webpack -w -d",
    "start": "npm run watch & browser-sync start --config bs-config.js",
    "build": "npm run pack && cp -f src/index.html build/"
  },
  "license": "MIT",
  "dependencies": {
    "alt": "^0.18.6",
    "alt-container": "^1.0.2",
    "alt-utils": "^1.0.0",
    "bulma": "^0.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "browser-sync": "^2.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25"
  }
}
